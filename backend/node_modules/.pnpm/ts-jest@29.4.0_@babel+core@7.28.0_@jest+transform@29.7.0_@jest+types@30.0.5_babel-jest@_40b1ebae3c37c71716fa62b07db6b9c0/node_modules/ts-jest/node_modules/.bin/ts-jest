#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@30.0.5_babel-jest@_40b1ebae3c37c71716fa62b07db6b9c0/node_modules/ts-jest/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@30.0.5_babel-jest@_40b1ebae3c37c71716fa62b07db6b9c0/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@30.0.5_babel-jest@_40b1ebae3c37c71716fa62b07db6b9c0/node_modules/ts-jest/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@30.0.5_babel-jest@_40b1ebae3c37c71716fa62b07db6b9c0/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
