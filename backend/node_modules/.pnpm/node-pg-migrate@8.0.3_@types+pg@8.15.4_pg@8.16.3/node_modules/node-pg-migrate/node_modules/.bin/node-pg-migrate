#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node-pg-migrate@8.0.3_@types+pg@8.15.4_pg@8.16.3/node_modules/node-pg-migrate/bin/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node-pg-migrate@8.0.3_@types+pg@8.15.4_pg@8.16.3/node_modules/node-pg-migrate/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node-pg-migrate@8.0.3_@types+pg@8.15.4_pg@8.16.3/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node-pg-migrate@8.0.3_@types+pg@8.15.4_pg@8.16.3/node_modules/node-pg-migrate/bin/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node-pg-migrate@8.0.3_@types+pg@8.15.4_pg@8.16.3/node_modules/node-pg-migrate/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node-pg-migrate@8.0.3_@types+pg@8.15.4_pg@8.16.3/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/node-pg-migrate.js" "$@"
else
  exec node  "$basedir/../../bin/node-pg-migrate.js" "$@"
fi
