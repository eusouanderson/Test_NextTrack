#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules/jest/bin/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules/jest/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules/jest/bin/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules/jest/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules:/home/eusouanderson/Projetos/Test_NextTrack/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../.pnpm/jest@29.7.0_@types+node@24.1.0_ts-node@10.9.2_@types+node@24.1.0_typescript@5.8.3_/node_modules/jest/bin/jest.js" "$@"
fi
